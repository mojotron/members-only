    "gulp": "^5.0.0",
    "gulp-copy": "^5.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@types/pg": "^8.11.10",
    "dotenv": "^16.4.5",
