  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/server.js",
    "build": "gulp build",
    "css:build": "postcss src/public/styles/tailwind.css -o src/public/styles/main.css",
    "dev": "node --loader ts-node/esm  --watch src/server.ts",
    "db:populate": "node ./dist/db/populate.js"
  },
  "keywords": [],
